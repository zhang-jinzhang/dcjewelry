// out: ../style.css, sourcemap: true, compress: true
@white: #fff;
@bgBlue: #446fcf;
@cGray: #999;
@cBlack: #333;
.fsc(@size: 14px, @line: 20px, @color: @cBlack) {
  font-size: @size;
  line-height: @line;
  color: @color;
}
.fsd {
  font-size: 14px;
  line-height: 20px;
  color: #333;
}
.arrows-r {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 20px;
  height: 20px;
  background: url(../img/icon.png) -100px -20px no-repeat;
}
.fs(@size: 14px, @line: 20px) {
  font-size: @size;
  line-height: @line;
}
.ts(@obj: transform, @s: .2s) {
  -webkit-transition: -webkit-@obj @s;
  transition: -webkit-@obj @s;
  -moz-transition: @obj @s,-moz-@obj @s;
  transition: @obj @s;
  transition: @obj @s,-webkit-@obj @s,-moz-@obj @s;
}
.tf(@obj){
  -webkit-transform: @obj;
  transition: @obj;
}
.omit {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.clamp(@num: 2) {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: @num;
  overflow: hidden;
}
.bg-gray {
  background: #f5f5f5;
}
.bg {
  background: #f5f5f5;
}
.bg-white {
