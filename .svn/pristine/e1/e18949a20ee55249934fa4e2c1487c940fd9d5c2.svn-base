package com.ceyi.project.dcjewelry.notice;

import pw.wecode.project.framework.jdbc.Column;
import pw.wecode.project.framework.jdbc.Table;

/**
 * Created by lingh on 2017/5/5.
 */
@Table("user_notice")
public class UserNotice {
    @Column(primaryKey = true, genrated = false)
    private int uid;
    @Column(primaryKey = true, genrated = false)
    private int nid;

    public int getUid() {
        return uid;
    }

    public void setUid(int uid) {
        this.uid = uid;
    }

    public int getNid() {
        return nid;
    }

    public void setNid(int nid) {
        this.nid = nid;
    }

    @Override
    public String toString() {
        return "UserNotice{" +
                "uid=" + uid +
                ", nid=" + nid +
                '}';
    }
}
